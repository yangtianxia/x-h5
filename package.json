    "@rollup/plugin-inject": "^5.0.5",
    "@types/extend": "^3.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.7.8",
    "@types/qs": "^6.9.16",
    "@types/shelljs": "^0.8.15",
    "@types/tinycolor2": "^1.4.6",
