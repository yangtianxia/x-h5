{
  "name": "x-mobile",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "pollen": "pollen --config pollen.config.mjs",
    "dev": "pnpm pollen && vite",
    "build": "vue-tsc -b && pnpm pollen && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint ./src --ext .ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "author": "yangtianxia <1304082170@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangtianxia/x-mobile.git"
  },
  "bugs": {
    "url": "https://github.com/yangtianxia/x-mobile/issues"
  },
  "homepage": "https://github.com/yangtianxia/x-mobile#readme",
